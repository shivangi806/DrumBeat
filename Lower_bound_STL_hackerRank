// lower_bound is used to return iterator
// It return the index of the element just greater than or equal to the key element
//********************************************************
#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int n;cin>>n;
    vector<int> v(n);
    for(int i=0;i<n;i++){
        cin>>v[i];
    }
    int q;
    cin>>q;
    while(q--){
        int key;
        cin>>key;
        int pos = lower_bound(v.begin(),v.end(),key)-v.begin();
        if(v[pos]==key){
            cout<<"Yes "<<pos+1<<endl;
        }
        else{
            cout<<"No "<<pos+1<<endl;
        }
    }
    
    return 0;
}
//***********************************************************
